<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ftpclient.controllers.LoginController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="163.0" layoutY="14.0" prefHeight="54.0" prefWidth="274.0" text="Login">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="loginLabel" layoutX="209.0" layoutY="80.0" prefHeight="30.0" prefWidth="182.0" promptText="login" text="admin" />
      <TextField fx:id="passwordLabel" layoutX="209.0" layoutY="139.0" prefHeight="30.0" prefWidth="182.0" promptText="password" text="admin" />
      <Button fx:id="loginButton" layoutX="221.0" layoutY="267.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="30.0" prefWidth="158.0" text="Login">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <TextField fx:id="serverLabel" layoutX="209.0" layoutY="206.0" prefHeight="30.0" prefWidth="182.0" promptText="server" text="localhost" />
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="9.0" layoutY="324.0" prefHeight="54.0" prefWidth="579.0" textFill="#fa0202">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
